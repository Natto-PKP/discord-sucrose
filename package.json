{
  "name": "discord.js-typescript",
  "version": "3.0.0",
  "description": "Structure for Typescript bot Discord",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "ts-node ./src/index.ts",
    "start:prod": "npm run build && node ./dist/index.js",
    "start:respawn": "ts-node-dev --respawn ./src/index.ts"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "discord.js",
    "javascript"
  ],
  "author": "Nat'",
  "license": "Apache-2.0 License",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "fs-extra": "^10.0.0"
  }
}
